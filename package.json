{
  "name": "turdquest",
  "version": "1.20.0",
  "description": "Electron port of TurdQuest",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder",
    "build:linux": "electron-builder --linux",
    "build:win": "electron-builder --win"
  },
  "author": "TheMilkMan",
  "license": "ISC",
  "devDependencies": {
    "electron": "^36.8.1",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2"
  },
  "build": {
    "icon": "icon-256.png",
    "productName": "TurdQuest",
    "appId": "com.themilkman.turdquest",
    "asar": true,
    "files": [
      "**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "category": "Game",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "electron-updater": "^6.6.2",
    "express": "^5.1.0",
    "fs-extra": "^11.3.1",
    "update-electron-app": "^3.1.1"
  }
}